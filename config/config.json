{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "off-canvas-nar-bar-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/offCanvasNarBar/OffCanvasNarBarApplicationCustomizer.js",
          "manifest": "./src/extensions/offCanvasNarBar/OffCanvasNarBarApplicationCustomizer.manifest.json"
        }
      ]
    },
    "off-canvas-spa-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/offCanvasSpa/OffCanvasSpaWebPart.js",
          "manifest": "./src/webparts/offCanvasSpa/OffCanvasSpaWebPart.manifest.json"
        }
      ]
    },
    "off-canvas-spa-2-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/offCanvasSpa2/OffCanvasSpa2WebPart.js",
          "manifest": "./src/webparts/offCanvasSpa2/OffCanvasSpa2WebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "boostrap": {
      "path": "https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css",
      "globalName": "boostrap"
    }
  },
  "localizedResources": {
    "OffCanvasNarBarApplicationCustomizerStrings": "lib/extensions/offCanvasNarBar/loc/{locale}.js",
    "OffCanvasSpaWebPartStrings": "lib/webparts/offCanvasSpa/loc/{locale}.js",
    "OffCanvasSpa2WebPartStrings": "lib/webparts/offCanvasSpa2/loc/{locale}.js"
  }
}
